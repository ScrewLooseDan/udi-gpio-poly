<nodeDefs>
    <nodeDef id="GPIO_HDR" nls="HDR">
        <editors />
        <sts>
			<st id="ST" editor="bool" />
		</sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="DISCOVER" />
            </accepts>
        </cmds>
    </nodeDef>
    <nodeDef id="GPIO_PIN" nls="PIN">
        <editors />
        <sts>
            <st id="ST" editor="GPIOST" />
            <st id="GV0" editor="GPIOMD" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="DON" />
                <cmd id="DOF" />
                <cmd id="QUERY" />
                <cmd id="SET_INPUT" />
                <cmd id="PULLUP" />
                <cmd id="PULLDOWN" />
            </accepts>
        </cmds>
    </nodeDef>
</nodeDefs>
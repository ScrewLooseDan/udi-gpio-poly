<nodeDefs>
    <nodeDef id="GPIO_HDR" nls="HDR">
        <editors />
        <sts>
            <st id="ST" editor="bool" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="DISCOVER" />
            </accepts>
        </cmds>
    </nodeDef>
    <nodeDef id="GPIO_PIN" nls="PIN">
        <editors />
        <sts>
            <st id="ST" editor="GPIOST" />
            <st id="GV0" editor="GPIOMD" />
            <st id="GV1" editor="PWMDC" />
            <st id="GV2" editor="PWMHZ" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="DON" />
                <cmd id="DOF" />
                <cmd id="QUERY" />
                <cmd id="SET_INPUT" />
                <cmd id="PULLUP" />
                <cmd id="PULLDOWN" />
                <cmd id="PWM" />
                <cmd id="PWMON">
                    <p id="D" editor="PWMDC" init="GV1" />
                    <p id="F" editor="PWMHZ" init="GV2" />
                </cmd>
                <cmd id="SET_DC">
                    <p id="" editor="PWMDC" />
                </cmd>
                <cmd id="SET_FREQ">
                    <p id="" editor="PWMHZ" />
                </cmd>
            </accepts>
        </cmds>
    </nodeDef>
</nodeDefs>